    {"id":"SIT_INVOICE_TC","name":"Invoice TC","regex":"","tc":["Finance"]},
    {"id":"SIT_EMP_INSURANCE_TC","name":"Employee Insurance Files TC","regex":"","tc":["HR","Finance"]},
    {"id":"SIT_LEGAL_TC","name":"Legal Affairs TC","regex":"","tc":["Legal"]},
    {"id":"SIT_AGREEMENTS_TC","name":"Agreements TC","regex":"","tc":["Legal","Finance"]},
    {"id":"SIT_DISCIPLINARY_TC","name":"Employee disciplinary action files TC","regex":"","tc":["HR"]},
    {"id":"SIT_FINANCE_TC","name":"Finance TC","regex":"","tc":["Finance"]},
    {"id":"SIT_SOURCE_CODE_TC","name":"Source Code TC","regex":"","tc":["IP"]},
    {"id":"SIT_BUDGET_TC","name":"Budget TC","regex":"","tc":["Finance"]},
    {"id":"SIT_IP_TC","name":"IP TC (intellectual property)","regex":"","tc":["IP"]},
    {"id":"SIT_MA_TC","name":"M and A Files TC","regex":"","tc":["Legal","Finance"]},
    {"id":"SIT_SOFTWARE_DEV_TC","name":"Software Product Development Files TC","regex":"","tc":["IP","Software"]},
    {"id":"SIT_PROJECT_DOCS_TC","name":"Project Documents TC","regex":"","tc":["Project"]},
    {"id":"SIT_BANK_STATEMENT_TC","name":"Bank Statement TC","regex":"","tc":["Finance"]},
    {"id":"SIT_LOAN_AGREEMENTS_TC","name":"Loan Agreements and offer letters TC","regex":"","tc":["Finance","HR"]},
    {"id":"SIT_WIRE_TRANSFER_TC","name":"Wire transfer TC","regex":"","tc":["Finance"]},
    {"id":"SIT_SOW_TC","name":"Statement of Work TC","regex":"","tc":["Project"]}
  ],
  "output": {
    "output_folder": "output/files",
    "mapping_file": "output/mapping.xlsx",
    "log_file": "output/generation.log"
  }
}
